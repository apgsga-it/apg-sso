{
    "name": "apg-sso-angular-app",
    "version": "0.1.0-SNAPSHOT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy-config.json --host 0.0.0.0 --live-reload false --disable-host-check",
        "startES5": "ng serve --configuration es5 --proxy-config proxy-config.json --host 0.0.0.0 --live-reload false --disable-host-check",
        "startProd": "ng serve --prod --proxy-config proxy-config.json --host 0.0.0.0 --live-reload false --disable-host-check",
        "build": "ng build",
        "prod": "ng build --prod",
        "test": "ng test",
        "testContinuous": "ng test --no-watch --browsers PhantomJS",
        "lint": "ng lint"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "11.1.0",
        "@angular/cdk": "11.1.0",
        "@angular/common": "11.1.0",
        "@angular/compiler": "11.1.0",
        "@angular/core": "11.1.0",
        "@angular/forms": "11.1.0",
        "@angular/material": "11.1.0",
        "@angular/platform-browser": "11.1.0",
        "@angular/platform-browser-dynamic": "11.1.0",
        "@angular/router": "11.1.0",
        "keycloak-angular": "8.1.0",
        "keycloak-js": "11.0.2",
        "rxjs": "6.6.3",
        "tslib": "^2.1.0",
        "zone.js": "0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1101.1",
        "@angular/cli": "11.1.1",
        "@angular/compiler-cli": "11.1.0",
        "@angular/language-service": "11.1.0",
        "@types/node": "14.14.22",
        "codelyzer": "^6.0.1",
        "ts-node": "8.10.2",
        "tslint": "~6.1.3",
        "typescript": "4.1.3"
    }
}
